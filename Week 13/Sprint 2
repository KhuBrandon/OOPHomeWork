#include<iostream>
#include<vector>
using namespace std;
int main() {
	vector<int> N;
	vector<int>::iterator it;
	for (int i = 0; i < 10; i++) {
		N.push_back(rand() % 100);
		cout << "Push back [" << i + 1 << "] : ";
		for (it = N.begin(); it != N.end(); it++) {
			cout << " " << *it;
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < 10; i++) {
		int ii = N[0];
		if (N.size() != 0) {
			N.erase(N.begin());
			cout << "Erase [" << i + 1 << "] : ";
			for (it = N.begin(); it != N.end(); it++) {
				cout << " " << *it;
			}
			cout << endl;
		}
	}
}
