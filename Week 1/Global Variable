void setup(){
  size(400,600);
  myitem(100,100,1);
  myitem(250,400,2);
}
void myitem(float a,float b,float c){
  fill(255,100,100);
  square(a-50*c,b-50*c,100*c);
  rect(a-35*c,b+50*c,20*c,25*c);
  rect(a+15*c,b+50*c,20*c,25*c);
  fill(255);
  circle(a,b-25*c,30*c);  
  fill(0);
  circle(a,b-25*c,15*c);
  rect(a-30*c,b+5*c,60*c,30*c);
  fill(255);
  rect(a-20*c,b+5*c,15*c,10*c);
  rect(a+5*c,b+5*c,15*c,10*c);
}
